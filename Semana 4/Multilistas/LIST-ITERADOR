std::list<int> miList;

for(int i=0; i<5; i++){
    miList.push_front(i+1);
    miList.push_back(i+1);
}

//ITERADOR

std::list<int>::iterator miIt;
for(miIt = miList.begin(); miIt != miList.end(); miIt++){
    std::cout <<
}

//REVERSA

std::list<int>::reverse_iterator rit;
for( rit=miList.rbegin(); rit!= miList.rend(); rit++ ){
    std::cout << *rit << std::endl;
}